{
  "name": "mapmyjourney",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -L index.ts --ignore ./tests",
    "start": "tsc && node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --config .prettierrc 'index.ts' 'src/**/*.ts' --write",
    "create-migration": "npx knex migrate:make --knexfile ./DbConfig.ts -x ts",
    "run-migrations": "npx knex migrate:latest --knexfile ./DbConfig.ts",
    "test:unit": "jest ./tests/unit/",
    "test:integration": "jest ./tests/integration/",
    "test:unit:coverage": "jest ./tests/unit/ --coverage --collectCoverageFrom=\"./src/**\"",
    "test:integration:coverage": "jest ./tests/integration/ --coverage --collectCoverageFrom=\"./src/**\"",
    "test:all:coverage": "jest ./tests/integration/ ./tests/unit/ --coverage --collectCoverageFrom=\"./src/**\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielabadi/MapMyJourney.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielabadi/MapMyJourney/issues"
  },
  "homepage": "https://github.com/danielabadi/MapMyJourney#readme",
  "devDependencies": {
    "@swc-node/core": "^1.10.2",
    "@swc-node/jest": "^1.6.3",
    "@swc/core": "^1.3.46",
    "@swc/jest": "^0.2.24",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "knex": "^2.4.2",
    "multer": "^1.4.5-lts.1",
    "nocache": "^3.0.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  }
}
